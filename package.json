{
  "name": "haniblox-resource",
  "version": "0.1.0",
  "description": "External resources for HaniBlox projects",
  "main": "start.js",
  "scripts": {
    "fetch": "rimraf external-resources download && node script/download.js --repo=suku11421/external-resources-v2",
    "fetch:cdn": "node script/download.js --repo=suku11421/external-resources-v2 --cdn=https://cdn.suku11421/",
    "start": "rimraf ../.openblockData/external-resources && node ./test/start.js",
    "clean": "rimraf external-resources dist translation downloading-* ../.openblockData/external-resources",
    "build:extension": "node script/generate-extension-page.js --url=https://suku11421.github.io/external-resources-v2/",
    "build:device": "node script/generate-device-page.js --url=https://suku11421.github.io/external-resources-v2/",
    "build": "rimraf dist && npm run build:extension && npm run build:device",
    "deploy": "npm run fetch && npm run build",
    "ext:opt-size": "opt-size --dir=external-resources",
    "ext:gen-config": "gen-config --dir=external-resources --version=v0.0.1 --repo=suku11421/external-resources-v2 --cdn=https://cdn.suku11421/",
    "ext:gen-hash": "gen-hash --dir=external-resources",
    "ext:i18n-extract": "i18n-extract  --dir=external-resources",
    "ext:i18n-push": "i18n-push --dir=external-resources",
    "ext:i18n-update": "i18n-update --dir=external-resources",
    "lint": "eslint ./",
    "test": "node test/upgrade.js"
  },
  "author": "my skill skool",
  "license": "MIT",
  "dependencies": {
    "compare-versions": "^3.6.0",
    "copy-dir": "^1.3.0",
    "express": "^4.17.1",
    "extract-zip": "^2.0.1",
    "folder-hash": "^4.0.1",
    "format-message": "^6.2.3",
    "fs-extra": "^10.0.0",
    "hash-files": "^1.1.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "require-all": "^3.0.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.3.0",
    "eslint-config-scratch": "^5.0.0",
    "haniblox-resource-cli": "github:suku11421/haniblox-resource-cli#main",
    "progress": "^2.0.3",
    "request": "^2.88.2",
    "request-progress": "^3.0.0",
    "transifex": "^1.6.6"
  }
}
